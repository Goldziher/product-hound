{
	"extensionBundle": {
		"id": "Microsoft.Azure.Functions.ExtensionBundle",
		"version": "[3.*, 4.0.0)"
	},
	"extensions": {
		"http": {
			"routePrefix": "api"
		}
	},
	"logging": {
		"applicationInsights": {
			"samplingSettings": {
				"isEnabled": true
			}
		},
		"logLevel": {
			"Function": "Debug",
			"Host.Aggregator": "Trace",
			"Host.Results": "Error",
			"default": "Information"
		}
	},
	"version": "2.0"
}
